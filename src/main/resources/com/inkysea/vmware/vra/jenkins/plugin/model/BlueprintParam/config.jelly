<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define"
         xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

        <f:entry title="vRealize Automation URL"  field="serverUrl">
            <f:textbox name="serverUrl" />
        </f:entry>

        <f:entry title="Tenant"  field="tenant">
             <f:textbox name="tenant" />
        </f:entry>

        <f:entry title="User Name"  field="userName">
             <f:textbox name="userName" />
        </f:entry>

        <f:entry title="Password"  field="password">
             <f:password name="password" />
        </f:entry>

         <f:entry title="Blueprint Path"  field="blueprintPath">
             <f:textbox name="blueprintPath" />
         </f:entry>

         <f:entry title="Publish Blueprint"  field="publishBlueprint">
              <f:checkbox default="true" name="publishBlueprint" />
         </f:entry>

         <f:entry title="Assign to Service Category" field="serviceCategory">
             <f:textbox name="serviceCategory" />
         </f:entry>


         <f:entry title="">
                  <div align="right">
                    <f:repeatableDeleteButton />
                  </div>
         </f:entry>

</j:jelly>